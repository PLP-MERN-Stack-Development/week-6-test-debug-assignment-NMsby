  "description": "Week 6: Testing and Debugging â€“ Ensuring MERN App Reliability",
  "main": "index.js",
  "scripts": {
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "install-all": "npm run install-client && npm run install-server",
    "test": "jest",
    "test:unit": "jest --selectProjects client server --testPathPattern=unit",
    "test:integration": "jest --selectProjects client server --testPathPattern=integration",
    "test:e2e": "cd client && npm run test:e2e",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "start": "cd server && npm start",
    "build": "cd client && npm run build",
    "setup-test-db": "cd server && npm run setup-test-db"
  },
