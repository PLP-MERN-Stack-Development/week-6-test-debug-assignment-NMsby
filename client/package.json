{
  "name": "client",
  "version": "1.0.0",
  "type": "module",
  "description": "React client for MERN testing assignment",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "preview": "vite preview",

    "test": "jest --config ../jest.config.js --selectProjects client --passWithNoTests",
    "test:unit": "jest --config ../jest.config.js --selectProjects client --testPathPattern=unit --passWithNoTests",
    "test:integration": "jest --config ../jest.config.js --selectProjects client --testPathPattern=integration --passWithNoTests",
    "test:watch": "jest --config ../jest.config.js --selectProjects client --watch --passWithNoTests",
    "test:coverage": "jest --config ../jest.config.js --selectProjects client --coverage --passWithNoTests",
    "test:e2e": "cypress run --headless",
    "test:e2e:open": "cypress open",
    "test:e2e:headless": "cypress run --headless",

    "cypress:install": "cypress install",
    "cypress:verify": "cypress verify",
    
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PLP-MERN-Stack-Development/week-6-test-debug-assignment-NMsby.git"
  },
  "private": true,
  "dependencies": {
    "axios": "^1.6.2",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@cypress/code-coverage": "^3.14.5",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@vitejs/plugin-react": "^4.2.0",
    "babel-jest": "^30.0.5",
    "cypress": "^13.17.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-junit": "^16.0.0",
    "jsdom": "^23.0.1",
    "nyc": "^17.1.0",
    "start-server-and-test": "^2.0.13",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
